# Tests for leetcode problem 1829

import main
import unittest


class TestMain(unittest.TestCase):
    def test_basic(self):
        s = main.Solution()
        self.assertEqual(s.getMaximumXor([0,1,1,3], 2), [0, 3, 2, 2])
        self.assertEqual(s.getMaximumXor([2,3,4,7], 3), [5, 2, 6, 5])

    


if __name__ == '__main__':
    unittest.main()
